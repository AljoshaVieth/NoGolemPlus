{
  "manifest_version": 3,
  "name": "No Golem Plus",
  "description": "Removes Golem Plus articles from the website.",
  "version": "1.0",
  "author": "Aljosha",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.golem.de/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}